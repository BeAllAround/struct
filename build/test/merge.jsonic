


basic: {
  in: [
    {a:1,b:2},
    {b:3,d:4},
  ]
  out: {
    a: 1, b: 3, d: 4
  }
}


cases: {
  set: [
    { in:
      [
        { a: 1, b: 2 },
        {
          b: 3,
          d: {
            e: 4,
            ee: 5
          },
          f: 6
        },
        {
          x: {
            y: { z: 7, zz: 8 }
          },
          q: { u: 9, uu: 10 },
          v: 11
        },
      ]
      out: {
        a: 1,
        b: 3,
        d: { e: 4, ee: 5 },
        f: 6,
        x: { y: { z: 7, zz: 8 } },
        q: { u: 9, uu: 10 },
        v: 11
      }
    }

    { in: [1,2], out: 2 }
    { in: [1,2,3], out: 3 }
    { in: [{},4], out: 4 }
    { in: [[],5], out: 5 }
    { in: [[],{}], out: {} }
    { in: [{},[]], out: [] }
    { in: [[6],{x:7}], out: {x:7} }
    { in: [{x:8},[9]], out: [9] }

    { in: [1,{a:11}], out: {a:11}}
    { in: [{},{a:{b:12}}], out: {a:{b:12}}}
    { in: [{},{},{a:{b:13}}], out: {a:{b:13}}}
    { in: [{},null,{a:{b:14}}], out: {a:{b:14}}}
    { in: [{},null,{a:{b:15}},true,[],{a:{b:16,c:null}}], out: {a:{b:16,c:null}}}
    { in: [[],{a:17}], out: {a:17}}
    { in: [{},[18]], out: [18]}
  ]
}


